{
  "version": 2,
  "buildCommand": "cd ../.. && pnpm turbo build --filter=@improbix/api",
  "installCommand": "cd ../.. && pnpm install",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.ts": {
      "maxDuration": 60
    }
  }
}
